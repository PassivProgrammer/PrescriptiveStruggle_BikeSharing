   "source": [
    "# Methode für die Tageszeit\n",
    "def get_time_of_day(x):\n",
    "    if(x >= 6 and x <10):\n",
    "        return \"Morning\"\n",
    "    elif(x >= 10 and x <12):\n",
    "        return \"Mid-morning\"\n",
    "    elif(x >= 12 and x <14):\n",
    "        return \"Noon\"\n",
    "    elif(x >= 14 and x <17):\n",
    "        return \"Afternoon\"\n",
    "    elif(x >= 17 and x <21):\n",
    "        return \"Evening\"\n",
    "    else:\n",
    "        return \"Night\""
   ]
