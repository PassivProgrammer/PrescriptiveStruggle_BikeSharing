    "# hinzufügen der Season\n",
    "def get_season(x):\n",
    "    if(x == 1 or x == 2 or x == 12):\n",
    "        return \"Winter\"\n",
    "    elif(x >= 3 and x <= 5):\n",
    "        return \"Spring\"\n",
    "    elif(x >= 6 and x <= 8):\n",
    "        return \"Sommer\"\n",
    "    else:\n",
    "        return \"Autumn\"\n",
    "\n",
    "df[\"season\"] = df[\"month\"].apply(lambda x: get_season(x))\n",
    "df.head(3)"
   ]
